/// {
///     description: 'multiline expr in else if condition',
///     data: {
///         foo: 42,
///         bar: 24
///     },
///     result: 'yes'
/// }

match / {
    if
        false()
    {
        "no"
    } else if
        .foo
        +
        .bar
        ==
        66
    {
        "yes"
    }
}

